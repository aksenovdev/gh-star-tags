{
    "name": "Github star tags",
    "description": "Tags for github stars",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "content_scripts": [
        {
          "matches": ["https://github.com/*tab=stars*"],
          "css": ["./assets/bundle.css"],
          "js": ["./assets/bundle.js"]
        }
    ]
  }
