{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "compilerOptions": {
        "moduleResolution": "Node",
        "types": ["chrome", "svelte"],
        "baseUrl": "./",
        "paths": {
            "@tags-api/*": ["src/tags-api/*"],
            "@storage-data": ["src/storage-data"],
        }
    },
    "include": ["src/**/*"],
    "exclude": [
        "node_modules/*",
        "__sapper__/*",
        "public/*"
    ]
}
